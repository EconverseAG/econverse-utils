{
  "name": "econverse-utils",
  "version": "1.0.0",
  "description": "a collection of utility for ecommerce development in vtex and shopify",
  "main": "lib/index.js",
  "author": "Eduardo Rodrigues <eduardo.rodrigues@econverse.com.br>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:EduardoRodriguesF/econverse-utils.git"
  },
  "exports": {
    ".": "./lib/index.js",
    "./vtex": "./vtex/index.js"
  },
  "scripts": {
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:utils": "jsdoc2md --files ./src/utils/**/*.ts -c jsdoc.conf.json > ./docs/utils.md",
    "docs:vtex": "jsdoc2md --files ./src/vtex/**/*.ts -c jsdoc.conf.json > ./docs/vtex.md",
    "docs": "yarn docs:utils && yarn docs:vtex",
    "build": "gulp build",
    "deploy": "gulp build && npm publish"
  },
  "keywords": [
    "utils",
    "econverse",
    "ecommerce",
    "shopify",
    "vtex"
  ],
  "files": [
    "lib/",
    "vtex/",
    "docs/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/EduardoRodriguesF/econverse-utils/issues"
  },
  "homepage": "https://github.com/EduardoRodriguesF/econverse-utils#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-strip": "^2.1.0",
    "@rollup/stream": "^2.0.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-loader": "^8.2.4",
    "babel-register": "^6.26.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tap": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^27.5.1",
    "jsdoc": "^3.6.10",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "map-stream": "^0.0.7",
    "merge-stream": "^2.0.0",
    "prettier": "^2.6.1",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.70.1",
    "toast-jsdoc": "^1.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "global": "^4.4.0"
  }
}
